
.PHONY = all clean

all:
	@(cd global; make)
	@(cd master_main; make)
	@(cd module; make)
	@(cd plugin; make)
	@(cd main; make)
	@(cd tools; make)
clean:
	@(cd global; make clean)
	@(cd master_main; make clean)
	@(cd module; make clean)
	@(cd plugin; make clean)
	@(cd main; make clean)
	@(cd tools; make clean)
